<h1>Ol√° Sequelize</h1>
<dic class="container">
    {{#each users}}
    <div class="item">
        <h2>{{this.name}}</h2>
        <p>{{this.occupation}}</p>
        <div class="actions">
        <a href="/users/{{this.id}}"class="details-btn">Veja mais</a>
        <form action="/users/delete/{{this.id}}" method="post">
        <input type="submit" value="Excluir" class="delete-btn">
        </form>
        </div>
    </div>
    {{/each}}
</dic>